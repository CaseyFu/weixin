<view class='container'>
    <view>{{url}}</view>
    <view>
        <canvas style="width:500rpx;height:500rpx" id="{{canvasId}}" canvas-id="{{canvasId}}" />
    </view>
    <form class="form" bindsubmit="getUrl">
        <input class="input-url" type="text" name="url" placeholder="{{url}}" />
        <view class="stuff"></view>
        <button class="submit-btn" form-type="submit" size="mini" type="primary">提交</button>
    </form>
</view>