/* pages/video/video.wxss */
.container {
    width: 100vw;
    min-height: 100vh;
    margin: 0;
    padding: 0;
    font-size: 16px;
    background-size: 20px;
    background-image: url('data:image/jpeg;base64,');
    background-repeat: repeat;
}

.item {
    margin-top: 20rpx;
    text-align: center;
    font-size: 0.7rem;
    font-weight: 300;
}

.item-inner {
    width: 100rpx;
    height: 100rpx;
    margin: 0 auto 5rpx auto;
}

.img {
    width: 100rpx;
    height: 100rpx;
}

.background {
    width: 100%;
    height: 100%;
    position: fixed;
    background-size: 100% 100%;
    z-index: -1;
}