<view class="container">
    <i-row>
        <i-col span="6" i-class="col-class item" wx:for="{{items}}" wx:key="index" wx:for-index="index" wx:for-item="item" bindtap="activate" data-param="{{item.pagePath}}">
            <view class="item-inner">
                <image class="img" src="/images/convey/{{item.image}}" />
            </view>
            <view>{{item.name}}</view>
        </i-col>
    </i-row>
</view>